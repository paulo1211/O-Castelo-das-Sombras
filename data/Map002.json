{"id":1,"name":"Cena","note":"","pages":[{"conditions":{"actorId":2,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"tileId":0,"characterName":"","direction":6,"pattern":0,"characterIndex":1},"list":[{"code":245,"indent":0,"parameters":[{"name":"Darkness","volume":100,"pitch":100,"pan":0}]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["Numa era de trevas e névoa, onde o silêncio da noite"]},{"code":401,"indent":0,"parameters":["era quebrado apenas pelo sussurro do vento,"]},{"code":401,"indent":0,"parameters":["uma entidade sombria emergia das profundezas da terra."]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["Sob o céu estrelado, o antigo rei, um homem de poder e "]},{"code":401,"indent":0,"parameters":["temor, enfrentava o ser de olhos ardentes.A lua cheia"]},{"code":401,"indent":0,"parameters":["iluminava o campo de batalha, revelando as ruínas de um"]},{"code":401,"indent":0,"parameters":["castelo outrora majestoso,agora palco de um confronto."]},{"code":223,"indent":0,"parameters":[[-68,-68,-68,0],60,true]},{"code":236,"indent":0,"parameters":["storm",5,60,true]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["Enquanto a noite se desdobrava, o rei e a entidade se"]},{"code":401,"indent":0,"parameters":["encaravam em um silêncio carregado de tensão."]},{"code":401,"indent":0,"parameters":["A entidade, uma sombra viva tecida com os fios do medo,"]},{"code":401,"indent":0,"parameters":["movia-se com uma graça perturbadora."]},{"code":236,"indent":0,"parameters":["none",5,60,true]},{"code":101,"indent":0,"parameters":["",0,0,1]},{"code":401,"indent":0,"parameters":["No frio da noite era cortado pelo som da espada do rei"]},{"code":401,"indent":0,"parameters":["ao ser desembainhada, um brilho prateado contra a escu"]},{"code":401,"indent":0,"parameters":["ridão. O rei, cuja coroa pesava mais do que o aço em"]},{"code":401,"indent":0,"parameters":["sua mão, sabia que o destino de seu povo dependia dele."]},{"code":201,"indent":0,"parameters":[0,6,10,11,0,0]},{"code":101,"indent":0,"parameters":["Monster",7,0,2]},{"code":401,"indent":0,"parameters":["Ah, a liberdade! Como é doce o ar que não"]},{"code":401,"indent":0,"parameters":["sentia há milênios. Vocês, mortais"]},{"code":401,"indent":0,"parameters":["audaciosos, não têm ideia do que acabaram"]},{"code":401,"indent":0,"parameters":["de desencadear. Por anos, estive confinada"]},{"code":101,"indent":0,"parameters":["Monster",7,0,2]},{"code":401,"indent":0,"parameters":["nas sombras, uma mera sussurro entre as"]},{"code":401,"indent":0,"parameters":["fendas dotempo, mas agora... agora eu"]},{"code":401,"indent":0,"parameters":["caminho entre vocês."]},{"code":201,"indent":0,"parameters":[0,7,13,11,2,0]},{"code":101,"indent":0,"parameters":["Rei Alden",0,0,2]},{"code":401,"indent":0,"parameters":["Sentem isso? Uma perturbação... como se"]},{"code":401,"indent":0,"parameters":["algo antigo tivesse despertado."]},{"code":101,"indent":0,"parameters":["Madame Morgana",0,0,2]},{"code":401,"indent":0,"parameters":["Sim, uma energia que não pertence a este"]},{"code":401,"indent":0,"parameters":["reino. Deve ser a entidade que foi invocada"]},{"code":101,"indent":0,"parameters":["PrincesaSeraphina",0,0,2]},{"code":401,"indent":0,"parameters":["Mas o que ela quer? Será que representa um"]},{"code":401,"indent":0,"parameters":["perigo para o reino?"]},{"code":101,"indent":0,"parameters":["Sir Draven",0,0,2]},{"code":401,"indent":0,"parameters":["Não devemos temer. Devemos estar preparados"]},{"code":401,"indent":0,"parameters":["para dialogar com a entidade e entender"]},{"code":401,"indent":0,"parameters":["suas intenções."]},{"code":101,"indent":0,"parameters":["Rei Alden",0,0,2]},{"code":401,"indent":0,"parameters":["Concordo, Sir Draven. Devemos abordar isso"]},{"code":401,"indent":0,"parameters":["com cautela e sabedoria. Madame Morgana,"]},{"code":401,"indent":0,"parameters":["você pode sentir a natureza dessa presença?"]},{"code":101,"indent":0,"parameters":["Madame Morgana",0,0,2]},{"code":401,"indent":0,"parameters":["É poderosa e antiga, mas não"]},{"code":401,"indent":0,"parameters":["necessariamente malévola. A entidade parece"]},{"code":401,"indent":0,"parameters":["estar explorando sua liberdade"]},{"code":401,"indent":0,"parameters":["recém-descoberta."]},{"code":101,"indent":0,"parameters":["PrincesaSeraphina",0,0,2]},{"code":401,"indent":0,"parameters":["Então talvez haja uma chance de "]},{"code":401,"indent":0,"parameters":["coexistência pacífica. Devemos estender"]},{"code":401,"indent":0,"parameters":["nossa hospitalidade e mostrar que não somos"]},{"code":401,"indent":0,"parameters":["inimigos."]},{"code":101,"indent":0,"parameters":["Sir Draven",0,0,2]},{"code":401,"indent":0,"parameters":["Eu me voluntario para ser o primeiro a"]},{"code":401,"indent":0,"parameters":["estabelecer contato. Se houver algum risco,"]},{"code":401,"indent":0,"parameters":["estarei na linha de frente para proteger"]},{"code":401,"indent":0,"parameters":["nosso reino."]},{"code":101,"indent":0,"parameters":["Sir Draven",0,0,2]},{"code":401,"indent":0,"parameters":["Princesa Sepharina, há algo que devo compartilhar."]},{"code":401,"indent":0,"parameters":["Lembra da antiga biblioteca que agora jaz em ruínas?"]},{"code":101,"indent":0,"parameters":["PrincesaSeraphina",0,0,2]},{"code":401,"indent":0,"parameters":["Sim, é uma perda lamentável."]},{"code":401,"indent":0,"parameters":["Tantos conhecimentos perdidos..."]},{"code":101,"indent":0,"parameters":["Sir Draven",0,0,2]},{"code":401,"indent":0,"parameters":["Era mantido sob chave e dizia-se que era"]},{"code":401,"indent":0,"parameters":["proibido lê-lo."]},{"code":101,"indent":0,"parameters":["Sir Draven",0,0,2]},{"code":401,"indent":0,"parameters":["Quando eu era apenas um menino, antes do"]},{"code":401,"indent":0,"parameters":["ataque dos demônios, eu costumava explorar"]},{"code":401,"indent":0,"parameters":["seus corredores secretos. Havia um"]},{"code":401,"indent":0,"parameters":["pergaminho que sempre me intrigou."]},{"code":101,"indent":0,"parameters":["PrincesaSeraphina",0,0,2]},{"code":401,"indent":0,"parameters":["Proibido? Por quais motivos?"]},{"code":101,"indent":0,"parameters":["Sir Draven",0,0,2]},{"code":401,"indent":0,"parameters":["Dizia-se que continha a verdadeira história"]},{"code":401,"indent":0,"parameters":["deste castelo, segredos que não eram para"]},{"code":401,"indent":0,"parameters":["ser conhecidos pelos moradores do reino."]},{"code":101,"indent":0,"parameters":["PrincesaSeraphina",0,0,2]},{"code":401,"indent":0,"parameters":["Isso é fascinante. E você acha que esse pergaminho"]},{"code":401,"indent":0,"parameters":["ainda pode estar lá?"]},{"code":101,"indent":0,"parameters":["Sir Draven",0,0,2]},{"code":401,"indent":0,"parameters":["É possível. Se ele sobreviveu ao ataque,"]},{"code":401,"indent":0,"parameters":["pode estar escondido entre as ruínas,"]},{"code":401,"indent":0,"parameters":["esperando para revelar seus segredos."]},{"code":101,"indent":0,"parameters":["PrincesaSeraphina",0,0,2]},{"code":401,"indent":0,"parameters":["Então devemos encontrar esse pergaminho,"]},{"code":401,"indent":0,"parameters":["Sir Draven. Pode ser a chave para"]},{"code":401,"indent":0,"parameters":["entendermos melhor nosso passado..."]},{"code":401,"indent":0,"parameters":["e talvez, preparar-nos para o futuro."]},{"code":101,"indent":0,"parameters":["Sir Draven",0,0,2]},{"code":401,"indent":0,"parameters":["Concordo, alteza. Com sua permissão, organizarei uma"]},{"code":401,"indent":0,"parameters":["expedição para recuperá-lo."]},{"code":101,"indent":0,"parameters":["PrincesaSeraphina",0,0,2]},{"code":401,"indent":0,"parameters":["Você tem minha permissão e meu apoio."]},{"code":401,"indent":0,"parameters":["Que a luz da sabedoria nos guie através das"]},{"code":401,"indent":0,"parameters":["sombras do esquecimento."]},{"code":101,"indent":0,"parameters":["Rei Alden",0,0,2]},{"code":401,"indent":0,"parameters":["Muito bem, Sir Draven. Prepare-se, e que os"]},{"code":401,"indent":0,"parameters":["antigos deuses nos guiem nesta nova jornada"]},{"code":401,"indent":0,"parameters":["que se inicia."]},{"code":214,"indent":0,"parameters":[]},{"code":234,"indent":0,"parameters":[1,[0,0,0,0],60,true]},{"code":201,"indent":0,"parameters":[0,1,0,19,0,0]},{"code":129,"indent":0,"parameters":[2,0,false]},{"code":129,"indent":0,"parameters":[5,1,false]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":1,"stepAnime":false,"through":false,"trigger":3,"walkAnime":true}],"x":16,"y":0}
